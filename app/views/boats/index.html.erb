<%= form_for Boat.new do |f| %>
	<%= f.label :name %><br>
	<%= f.text_field :name %><br>
	<%= f.label :containers %><br>
	<%= f.text_field :containers %><br>
	<%= f.label :location %><br>
	<%= f.text_field :location %><br>
	<%= f.submit "Add boat" %>
<%end%>
<%= current_user.id %>

<% @boat.each do |boat| %>
	<%= link_to boat.name,  boat_path(boat) %>
	<br> 
<% end %>